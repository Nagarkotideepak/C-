//Linear Search
#include<iostream>
using namespace std;
int main(){
	int A[10],n=10,key;
	cout<<"Enter numbers: "<<endl;
	for(int i=0;i<n;i++){
		cin>>A[i];	
	}
	cout<<"Enter Key: "<<endl;
	cin>>key;
	for(int i=0;i<n;i++){
		if(key==A[i]){
			cout<<"Key is found at "<<i;
			return 0;
		}
	}
	cout<<"Not Found!!!";
	return 0;
}
