// Nature of quadratic Equation
#include <iostream>
#include<math.h>
using namespace std;
int main(){
	float a,b,c,d,r1,r2;
	cout<<"Enter a,b and c:"<<endl;
	cin>>a>>b>>c;
	d= b*b - 4*a*c;
	if(d=0){
		cout<<"Real and Equal"<<endl;
		cout<<(-b/(2*a));
	}
	else if(d>0){
		cout<<"Real and Unequal"<<endl;
		cout<<(-b+sqrt(d)/(2*a));
	}
	else{
		cout<<"Imaginary"<<endl;
		cout<<(-b-sqrt(d)/(2*a));
	}
	
	return 0;
}
