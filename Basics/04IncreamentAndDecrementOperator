#include <iostream>
using namespace std;
int main()
{
int i=5,j;
 j=i++;//post increament
 cout<<j<<" "<<i<<endl;
int k=5,l;
 l=++k;//pre increament
 cout<<l<<" "<<k<<endl;
int a=5,b;
 b=2*++a + 2*a++;// 2* 6 + 2*7
 cout<<b<<" "<<a<<endl;;
int c=5,d;
 d=2*c++ + 2*c++; // 2*5+2*6
 cout<<d<<" "<<c<<endl;
return 0;
}
