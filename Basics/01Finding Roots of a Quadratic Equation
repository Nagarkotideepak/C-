#include<iostream>
#include<math.h>
using namespace std;
int main(){
	cout<<"!!!Finding Roots of a Quadratic Equation!!"<<endl;
	float a,b,c,r1,r2;
	cout<<"Enter the value of a,b,c of equations:"<<endl;
	cin>>a>>b>>c;
	r1=(-b+sqrt(b*b-4*a*c))/(2*a);
	r2=(-b-sqrt(b*b-4*a*c))/(2*a);
	cout<<"Roots of the your equation are : "<<r1<<" "<<r2;
    return 0;
}
